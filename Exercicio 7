#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>

main(){
	
      int  i    = 0,
           num  = 0,
           soma = 0;
         
        setlocale(LC_ALL, "Portuguese");
         
        for(i = 1; i <= 10; i++)
		    {         	
         	  printf("Digite o %dº numero:", i);
         	  scanf("%d", &num);
         	
         	  soma = soma + num;
        }
         
        printf("A soma dos dez números: %d", soma);
         
    return 0; 
}
